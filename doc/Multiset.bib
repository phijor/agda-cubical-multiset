@Article{Ahrens2015,
  author        = {Ahrens, Benedikt and Capriotti, Paolo and Spadotti, Régis},
  date          = {2015},
  title         = {{N}on-wellfounded trees in {H}omotopy {T}ype {T}heory},
  doi           = {10.4230/LIPICS.TLCA.2015.17},
  eprint        = {1504.02949},
  language      = {en},
  pages         = {17--30},
  series        = {LIPIcs},
  volume        = {38},
  abstract      = {We prove a conjecture about the constructibility of coinductive types - in the principled form of indexed M-types - in Homotopy Type Theory. The conjecture says that in the presence of inductive types, coinductive types are derivable. Indeed, in this work, we construct coinductive types in a subsystem of Homotopy Type Theory; this subsystem is given by Intensional Martin-L\"of type theory with natural numbers and Voevodsky's Univalence Axiom. Our results are mechanized in the computer proof assistant Agda.},
  archiveprefix = {arXiv},
  booktitle     = {Proceedings of TLCA 2015},
  file          = {:Ahrens2015 - Non Wellfounded Trees in Homotopy Type Theory.pdf:PDF},
  keywords      = {cs.LO, math.CT, F.3.2},
  month         = apr,
  primaryclass  = {cs.LO},
  year          = {2015},
}

